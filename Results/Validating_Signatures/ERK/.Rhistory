boxplot_erk =function(colnumber) {
sensitive = merge(data_erk[colnumber],erk_sensitive,by.x=0, by.y=1)
print("sensitive")
print(sensitive)
resistant = merge(data_erk[colnumber], erk_resistent, by.x=0, by.y=1)
print("resistant") ; print(resistant)
sensitive_cor = cor(sensitive[,2], as.numeric(sensitive[,3]), use="na.or.complete",method= "spearman")
print("Correlation between sensitive cells line predictions and drug sensitivity")
print(sensitive_cor)
resistant_cor = cor(resistant[,2],  as.numeric(resistant[,3]), use="na.or.complete",method= "spearman") # 0.28
print("Correlation between 'resistant' cells line predictions and drug sensitivity")
print(resistant_cor)
ttest= t.test(resistant[,2],sensitive[,2])
pval = ttest[3]
print("pval for resistant vs. sensitive pathway predictions")
print(pval)
#boxplot(resistant[,2],sensitive[,2], names=c("Resistant","Sensitive"), ylab= "pathway activity", main= gsub("_multi/pathway_activity_testset.csv", "", colnames(data_erk[colnumber])))
}
boxplot_erk(1)
